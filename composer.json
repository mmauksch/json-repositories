{
    "name": "mmauksch/json-repositories",
    "description": "Basic implementation of the repository pattern that stores its content in serialized json files. Intended use is for testing or prototyping! NOT INTENDED FOR PRODUCTION USE!",
    "keywords": ["json", "repository", "prototyping"],
    "version": "1.5.0",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "symfony/serializer": "*",
        "symfony/finder": "*",
        "symfony/filesystem": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "symfony/property-info": "*",
        "phpdocumentor/reflection-docblock": "*",
        "symfony/property-access": "*"
    },
    "autoload": {
        "psr-4": {
            "Mmauksch\\JsonRepositories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmauksch\\JsonRepositories\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Markus Mauksch",
            "email": "markus.mauksch@ea-energie.de"
        },
        {
          "name": "Alina Scholz",
          "email": "alina.scholz@ea-energie.de"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
